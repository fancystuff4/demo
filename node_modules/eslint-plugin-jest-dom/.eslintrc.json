{
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
        // "jest/globals": true
    },
    "plugins": [
        "eslint-plugin",
        "prettier"
    ],
    "extends": [
        "plugin:eslint-plugin/recommended"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-var": "error"
    }
}